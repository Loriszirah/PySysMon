<div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
  <h1 class="page-header">Dashboard</h1>

  <div class="row">
    <h2>Machines</h2>
    <% machines.forEach(function(machine , index) { %>
        <div class="col-xs-6 col-sm-3">
          <img src="/images/ordinateur.png" class="img-rounded img-responsive">

              <h4><%= machine.NomMachine %></h4>

          <span class="text-muted">IP : <%= machine.IPMachine %></span>

        </div>
    <% }); %>
  </div>
</div>
