<div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
  <h1 class="page-header">Machines</h1>

  <div class="row">
    <% machines.forEach(function(machine , index) { %>
    <a href="<%= machine.IDMachine %>">
        <div class="text-center col-xs-6 col-sm-3">
          <img src="/images/ordinateur.png" width="200" height="200" class="center-block img-rounded img-responsive">

          <h4><%= machine.NomMachine %></h4>

          <span class="text-muted">IP : <%= machine.IPMachine %></span>

        </div>
    </a>
    <% }); %>
  </div>
</div>
